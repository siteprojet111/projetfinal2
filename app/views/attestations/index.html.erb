<p id="notice"><%= notice %></p>

<h1>Attestations</h1>

<table>
  <thead>
    <tr>
      <th>Titre attes</th>
      <th>Universite</th>
      <th>Date da</th>
      <th>Date fa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attestations.each do |attestation| %>
      <tr>
        <td><%= attestation.titre_attes %></td>
        <td><%= attestation.universite %></td>
        <td><%= attestation.date_da %></td>
        <td><%= attestation.date_fa %></td>
        <td><%= link_to 'Show', attestation %></td>
        <td><%= link_to 'Edit', edit_attestation_path(attestation) %></td>
        <td><%= link_to 'Destroy', attestation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attestation', new_attestation_path %>
